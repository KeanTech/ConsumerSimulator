@page "/"

<UiContainerComponent
Manager="Manager"
ConsumerProgress="Manager.Consumer.Items.Count"
ProducerProgress="Manager.Producer.Items.Count">
</UiContainerComponent>


@code
{
    Manager Manager;

    protected override void OnInitialized()
    {
        Manager = new Manager();
        Manager.UpdateConsumer += (o, e) => StateHasChanged();
        Manager.UpdateProducer += (o, e) => StateHasChanged();
    }

}